  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch In Focus",
      "program": "${file}",
      "cwd": "${fileDirname}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch with CMD args",
      "program": "${file}",
      "args": [
        "2021",
        "15"
      ],
      "cwd": "${fileDirname}",
      "console": "integratedTerminal" 
    }
  ]
}